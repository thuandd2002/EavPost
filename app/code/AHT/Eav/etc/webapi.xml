<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/eav/post" method="GET" >
        <service class="AHT\Eav\Api\PostManagementInterface" method="getPost"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
     <route url="/V1/eav/categories" method="GET" >
        <service class="AHT\Eav\Api\CategoriesManagementInterface" method="getCategories"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
